let g:mysys = 'mac'
let g:vim_local = '~/.vim'

"Source the vimrc file from an arbitrary location.
"Not necessary if the repo is already installed in VIM's runtime path.
exec 'set runtimepath^='.g:vim_local
exec 'source '.g:vim_local.'/vimrc'

"let Tlist_Inc_Winwidth = 1
"let Tlist_Ctags_Cmd = "/usr/local/ctags/bin/ctags"
